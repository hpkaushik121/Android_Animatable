<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Animateable">
        <attr name="animation" format="integer">
            <enum name="bounce" value="1" />
            <enum name="bounceInRight" value="2" />
            <enum name="bounceInLeft" value="3" />
            <enum name="bounceInUp" value="4" />
            <enum name="bounceInDown" value="5" />
            <enum name="flash" value="6" />
            <enum name="rubberBand" value="7" />
            <enum name="pulse" value="8" />
            <enum name="shake" value="9" />
            <enum name="swing" value="10" />
            <enum name="wobble" value="11" />
            <enum name="tada" value="12" />
            <enum name="standUp" value="13" />
            <enum name="wave" value="14" />
            <enum name="rollin" value="15" />
            <enum name="rollOut" value="16" />
            <enum name="bounceIn" value="17" />
            <enum name="fadeInUp" value="18" />
            <enum name="fadeInDOwn" value="19" />
            <enum name="fadeInLeft" value="20" />
            <enum name="fadeInRight" value="21" />
            <enum name="fadeOut" value="22" />
            <enum name="fadeOutUp" value="23" />
            <enum name="fadeOutDown" value="24" />
            <enum name="fadeOutRight" value="25" />
            <enum name="fadeOutLeft" value="26" />
            <enum name="flipInX" value="27" />
            <enum name="flipInY" value="28" />
            <enum name="flipOutX" value="29" />
            <enum name="flipOutY" value="30" />
            <enum name="rotateIn" value="31" />
            <enum name="rotateInDownLeft" value="32" />
            <enum name="rotateInDownRight" value="33" />
            <enum name="rotateInUpLeft" value="34" />
            <enum name="rotateInUpRight" value="35" />
        </attr>
        <attr name="autoStart" format="boolean"/>
        <attr name="startDelay" format="integer"/>
        <attr name="repeatCount" format="integer">
        </attr>
        <attr name="repeatMode" format="integer">
            <enum name="INFINITE" value="-1"/>
            <enum name="RESTART" value="1" />

            <enum name="REVERSE" value="2" />

            <enum name="START_ON_FIRST_FRAME" value="-1" />

            <enum name="ABSOLUTE" value="0" />

            <enum name="RELATIVE_TO_SELF" value="1" />

            <enum name="RELATIVE_TO_PARENT" value="2" />

            <enum name="ZORDER_NORMAL" value="0" />

            <enum name="ZORDER_TOP" value="1" />


            <enum name="ZORDER_BOTTOM" value="-1" />
        </attr>
        <attr name="duration" format="integer"/>
    </declare-styleable>
</resources>